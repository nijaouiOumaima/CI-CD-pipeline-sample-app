pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                // Get some code from a GitHub repository
                git url: 'https://github.com/nijaouiOumaima/CI-CD-pipeline-sample-app.git', branch: 'main'
           
             
            }
        }
        stage ('Test') {
             steps { 
                echo 'hello test'
 }
}
    }
}
